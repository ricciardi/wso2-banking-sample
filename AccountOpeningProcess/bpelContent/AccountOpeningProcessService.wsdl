<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns:tns="http://wso2.org/banking" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:cs="http://sample.wso2.org"
	xmlns:tns1="http://wso2.org/banking/xsd"
	name="AccountOpeningProcessService" targetNamespace="http://wso2.org/banking" xmlns:xsd1="http://sample.wso2.org/xsd" xmlns:xsd2="http://sample.wso2.org/">
	<wsdl:types>
		<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="http://wso2.org/banking/xsd"
				schemaLocation="AccountOpeningProcessTypes.xsd">
			</xsd:import>
			<xsd:import namespace="http://sample.wso2.org/"
				schemaLocation="CardTypes.xsd">
			</xsd:import></xsd:schema></wsdl:types>
	<wsdl:message name="CreateAccountRequest">
		<wsdl:part element="tns1:CreateAccount" name="parameters" />
	</wsdl:message>
	<wsdl:message name="CreateAccountResponse">
		<wsdl:part element="xsd2:issueCardResponse" name="parameters" />
	</wsdl:message>
	<wsdl:portType name="AccountOpeningProcessService">
		<wsdl:operation name="CreateAccount">
			<wsdl:input message="tns:CreateAccountRequest" />
			<wsdl:output message="tns:CreateAccountResponse" />
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="AccountOpeningProcessServiceSOAP"
		type="tns:AccountOpeningProcessService">
		<soap:binding style="document"
			transport="http://schemas.xmlsoap.org/soap/http" />
		<wsdl:operation name="CreateAccount">
			<soap:operation soapAction="http://wso2.org/banking/NewOperation" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="AccountOpeningProcessService">
		<wsdl:port binding="tns:AccountOpeningProcessServiceSOAP"
			name="AccountOpeningProcessServiceSOAP">
			<soap:address location="http://www.example.org/" />
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
